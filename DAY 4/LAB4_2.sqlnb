  - kind: 2
    value: "SELECT ENAME FROM EMP_39\r

      WHERE SUBSTR(LOWER(ENAME),2,1) ='i';"
    languageId: oracle-sql
  - kind: 2
    value: "SELECT DISTINCT MGR\r\nFROM EMP_39;"
    languageId: oracle-sql
  - kind: 2
    value: "SELECT *\r

      FROM EMP_39\r

      WHERE EMPNO NOT IN (\r

      \    SELECT DISTINCT MGR\r

      \    FROM EMP_39\r

      \    WHERE MGR IS NOT NULL\r

      );\r\n"
    languageId: oracle-sql
  - kind: 2
    value: "SELECT *\r

      FROM EMP_39\r

      WHERE JOB NOT lIKE 'M%';\r\n"
    languageId: oracle-sql
  - kind: 2
    value: "SELECT MAX(SAL) AS \"MAX SALARY\",\r

      \    MIN(SAL) AS \"MIN SALARY\",\r

      \    SUM(SAL) AS \"TOTAL SALARY\",\r

      \    AVG(SAL) AS \"AVERAGE SALARY\"\r

      \    FROM EMP_39;"
    languageId: oracle-sql
  - kind: 2
    value: "SELECT MAX(SAL) AS \"MAX SALARY\",\r

      \    MIN(SAL) AS \"MIN SALARY\",\r

      \    SUM(SAL) AS \"TOTAL SALARY\",\r

      \    AVG(SAL) AS \"AVERAGE SALARY\",\r

      \    JOB\r

      \    FROM EMP_39\r

      \    GROUP BY JOB;"
    languageId: oracle-sql
  - kind: 2
    value: "SELECT COUNT(*) AS \"Number of managers\"\r

      FROM EMP_39\r

      WHERE JOB lIKE 'M%';\r\n"
    languageId: oracle-sql
  - kind: 2
    value: "SELECT *\r

      FROM EMP_39\r

      WHERE HIREDATE > TO_DATE('01-JAN-1980', 'DD-MON-YYYY');"
    languageId: oracle-sql
  - kind: 2
    value: "SELECT MAX(SAL)\r

      FROM EMP_39\r

      WHERE JOB LIKE 'SALESMAN';"
    languageId: oracle-sql
  - kind: 2
    value: "SELECT *\r

      FROM EMP_39\r

      WHERE JOB LIKE 'SALESMAN'\r

      AND SAL = (SELECT MAX(SAL)\r

