    languageId: oracle-sql
  - kind: 2
    value: "SELECT SAL \r

      \    FROM EMP_39\r

      \    WHERE ENAME = 'TURNER';"
    languageId: oracle-sql
  - kind: 2
    value: "SELECT *\r

      FROM EMP_39\r

      WHERE SAL > (\r

      \    SELECT SAL \r

      \    FROM EMP_39\r

      \    WHERE ENAME = 'TURNER'\r

      ) \r

      ORDER BY SAL DESC;"
